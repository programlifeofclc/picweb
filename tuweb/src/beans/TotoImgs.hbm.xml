<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="beans.TotoImgs" table="toto_imgs">
<id column="id" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="img_name" length="20" name="imgName" type="java.lang.String"/>
<property column="img_context" length="100" name="imgContext" type="java.lang.String"/>
<property column="img_url" length="255" name="imgUrl" type="java.lang.String"/>
<property column="create_time" length="19" name="createTime" not-null="true" type="java.util.Date"/>
<property column="img_width" length="11" name="imgWidth" type="java.lang.Integer"/>
<property column="img_height" length="11" name="imgHeight" type="java.lang.Integer"/>
<property column="img_uploader" length="20" name="imgUploader" type="java.lang.String"/>
</class>
</hibernate-mapping>
